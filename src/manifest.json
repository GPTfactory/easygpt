{
  "name": "Search GPT",
  "version": "1.0.0",
  "description": "ChatGPT as a search engine",
  "manifest_version": 3,
  "author": "Wassim Ben Jdida",
  "action": {
    "default_popup": "index.html",
    "default_title": "SearchGPT"
  },
  "icons": { "16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon120.png" },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["*://*/*"]
    }
  ]
}
